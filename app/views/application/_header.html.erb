    <% if user_signed_in? %>
    <div class="Polaris-TopBar__Contents">
      <div>
        <div class="Polaris-TopBar-Menu__ActivatorWrapper">
          <%= button_to publics_path, method: 'get', class: 'Polaris-Button Polaris-Button--primary', style: 'margin-top: auto', form: { style: 'padding: 0; margin-right: 20px;' } do %>
            <span class="Polaris-Button__Content">
              <span class="Polaris-Button__Text">
                Check out Trends!
              </span>
            </span>
          <% end %>
          <a href="<%= user_path(current_user) %>" style="color: white;">
            <button id="user-button" type="button" class="Polaris-TopBar-Menu__Activator" tabindex="0" aria-controls="Polarispopover2" aria-owns="Polarispopover2" aria-expanded="false">
              <div class="Polaris-MessageIndicator__MessageIndicatorWrapper">
                <span aria-label="Avatar with initials D" role="img" class="Polaris-Avatar Polaris-Avatar--sizeSmall Polaris-Avatar--styleThree">
                  <span class="Polaris-Avatar__Initials">
                    <svg class="Polaris-Avatar__Svg" viewBox="0 0 40 40">
                      <text x="50%" y="50%" dy="0.35em" fill="currentColor" font-size="20" text-anchor="middle"><%= current_user.name.slice(0, 1).upcase %></text>
                    </svg>
                  </span>
                </span>
              </div>
              <span class="Polaris-TopBar-UserMenu__Details">
                <p class="Polaris-TopBar-UserMenu__Name"><%= current_user.name %></p>
                <% if current_user.admin %>
                  <p class="Polaris-TopBar-UserMenu__Detail">Admin</p>
                <% end %>
              </span>
            </button>
          </a>
        </div>
      </div>
    </div>
